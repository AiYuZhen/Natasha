<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;net462;net472;netstandard2.0;netcoreapp3.0;netcoreapp3.1;net5.0;</TargetFrameworks>
    <Description>Natasha 编译域的实现</Description>
    <PackageId>DotNetCore.Natasha.Domain</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>兼容 .NET5,增加强签名, 升级依赖</PackageReleaseNotes>
    <Version>2.4.0.1</Version>
    <PackageLicenseUrl>https://github.com/dotnetcore/Natasha/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dotnetcore/Natasha</PackageProjectUrl>
    <PackageTags>Roslyn;IL;Scripte;Dynamic;Natasha;Domain;</PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/19404084</PackageIconUrl>
    <FileVersion>2.4.0.1</FileVersion>
    <AssemblyVersion>2.4.0.1</AssemblyVersion>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>natasha.pfx</AssemblyOriginatorKeyFile>
  </PropertyGroup>



	<ItemGroup>
		<ProjectReference Include="..\Natasha.Framework\Natasha.Framework.csproj" />
	</ItemGroup>




</Project>
