<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>netstandard2.0;netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netcoreapp3.0;netcoreapp3.1;net5.0;</TargetFrameworks>
	  <Description>隐藏掉第一个方法中第一个参数或者指定参数的调用头.</Description>
	  <PackageId>Natasha.CSharp.Extension.InvisibleInstance</PackageId>
	  <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	  <PackageReleaseNotes>使用语义分析截取 CS0103 错误信息进行节点替换</PackageReleaseNotes>
	  <Version>1.1.3.1</Version>
	  <PackageLicenseUrl>https://github.com/dotnetcore/Natasha/blob/master/LICENSE</PackageLicenseUrl>
	  <PackageProjectUrl>https://github.com/dotnetcore/Natasha</PackageProjectUrl>
	  <PackageTags>Roslyn;IL;Script;Dynamic;Natasha;NMS;CS0104;</PackageTags>
	  <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	  <PackageIconUrl>https://avatars2.githubusercontent.com/u/51699821</PackageIconUrl>
	  <FileVersion>1.1.3.1</FileVersion>
	  <AssemblyVersion>1.1.3.1</AssemblyVersion>
	  <Copyright>.NET Core Community and Contributors</Copyright>
	  <Owners>.NET Core Community and Contributors</Owners>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Natasha.CSharp.All\Natasha.CSharp.All.csproj" />
  </ItemGroup>
	
</Project>
