<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netcoreapp3.0;netcoreapp3.1;net5.0;</TargetFrameworks>
    <Description>Natasha 编译框架以及各个操作类的基础类</Description>
    <PackageId>DotNetCore.Natasha.Framework</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>增加 NETCOREAPP 3.1 .NET5 Runtime 支持 ,增加强签名</PackageReleaseNotes>
    <Version>2.5.0</Version>
    <PackageLicenseUrl>https://github.com/dotnetcore/Natasha/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dotnetcore/Natasha</PackageProjectUrl>
    <PackageTags>Roslyn;IL;Scripte;Dynamic;NCC;Framework;Natasha;</PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <FileVersion>2.5.0.0</FileVersion>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/19404084</PackageIconUrl>
    <AssemblyVersion>2.5.0.0</AssemblyVersion>
    <Copyright>.NET Core Community and Contributors</Copyright>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>natasha.snk</AssemblyOriginatorKeyFile>
    <!--<PackageIcon>19404084.png</PackageIcon>
    <PackageLicenseFile>License.txt</PackageLicenseFile>-->
  </PropertyGroup>

  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetCore.SourceLink.Environment" Version="2.2.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="3.10.0" />
  </ItemGroup>

  
</Project>
