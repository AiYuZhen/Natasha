<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>NET461;netstandard2.0;netcoreapp3.0;</TargetFrameworks>
    <Description>Natasha 编译框架以及各个操作类的标准</Description>
    <PackageId>DotNetCore.Natasha.Framework</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>发布预览版</PackageReleaseNotes>
    <Version>1.3.0-alpha1</Version>
    <PackageLicenseUrl>https://github.com/dotnetcore/Natasha/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dotnetcore/Natasha</PackageProjectUrl>
    <PackageTags>Roslyn;IL;Scripte;Dynamic;NCC;Framework;Natasha;</PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <FileVersion>1.3.0.0</FileVersion>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/19404084</PackageIconUrl>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
    <Copyright>.NET Core Community and Contributors</Copyright>
    <!--<PackageIcon>19404084.png</PackageIcon>
    <PackageLicenseFile>License.txt</PackageLicenseFile>-->
  </PropertyGroup>
  
  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
  </ItemGroup>



  <ItemGroup>
    <PackageReference Include="DotNetCore.Compile.Environment" Version="2.0.0" />
    <PackageReference Include="DotNetCore.SourceLink.Environment" Version="1.0.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="3.7.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyModel" Version="3.1.6" />
  </ItemGroup>

  
</Project>
