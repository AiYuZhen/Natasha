<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netcoreapp3.0;</TargetFrameworks>
    <Description>Natasha C#编译引擎</Description>
    <PackageId>DotNetCore.Natasha.CSharpEngine</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>发布预览版</PackageReleaseNotes>
    <Version>1.1.0</Version>
    <PackageLicenseUrl>https://github.com/dotnetcore/Natasha/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dotnetcore/Natasha</PackageProjectUrl>
    <PackageTags>Roslyn ; IL ; Scripte ; Dynamic ; </PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/19404084</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetCore.Natasha.CSharpCompiler" Version="1.0.2.1" />
    <PackageReference Include="DotNetCore.Natasha.CSharpSyntax" Version="1.0.0.2" />
    <PackageReference Include="DotNetCore.Natasha.Domain" Version="1.1.0" />
  </ItemGroup>


</Project>
